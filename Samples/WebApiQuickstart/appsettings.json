{
  "DotNetSecurityToolkit": {
    "SecretKey": "replace-with-32-char-secret-key-or-longer",
    "PasswordHashIterations": 120000,
    "PasswordSaltSize": 16,
    "PasswordKeySize": 32,
    "Jwt": {
      "Issuer": "webapi-quickstart",
      "Audience": "webapi-quickstart-clients",
      "SigningKey": "replace-with-32-char-signing-key",
      "AccessTokenExpirationMinutes": 90,
      "ValidateLifetime": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
